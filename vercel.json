{
    "version": 2,
    "builds": [
      {
        "src": "api/app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
        { 
            "source": "/api/auth/(.*)", 
            "destination": "/api/auth/$1" 
        },

        {
            "source": "/api/(.*)",
            "destination": "api/app.py"
        }
    ]
  }